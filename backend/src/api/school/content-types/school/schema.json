{
  "kind": "collectionType",
  "collectionName": "schools",
  "info": {
    "singularName": "school",
    "pluralName": "schools",
    "displayName": "School"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "schoolNumber": {
      "type": "integer",
      "required": true
    },
    "schedule": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::schedule.schedule",
      "inversedBy": "school"
    },
    "donors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::donor.donor",
      "mappedBy": "schools"
    },
    "sponsors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sponsor.sponsor",
      "inversedBy": "schools"
    }
  }
}
